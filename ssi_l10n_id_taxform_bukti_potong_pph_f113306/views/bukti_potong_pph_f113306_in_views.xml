<record id="bukti_potong_pph_f113306_in_view_form" model="ir.ui.view">
    <field name="name">l10n_id.bukti_potong_pph_f113306_in - Form</field>
    <field name="model">l10n_id.bukti_potong_pph_f113306_in</field>
    <field name="mode">primary</field>
    <field name="priority">100</field>
    <field
            name="inherit_id"
            ref="ssi_l10n_id_taxform_bukti_potong_pph_mixin.bukti_potong_pph_mixin_view_form"
        />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//sheet" position="inside">
                <h1 name="form_name" />
            </xpath>
            <xpath expr="//field[@name='income_move_line_ids']" position="replace">
                <field
                        name="income_move_line_ids"
                        nolabel="1"
                        domain="[('id','in',allowed_income_move_line_ids)]"
                    >
                    <tree string="Journal Items">
                        <field name="date" />
                        <field
                                name="move_id"
                                required="0"
                                groups="account.group_account_user"
                            />
                        <field name="journal_id" options='{"no_open":True}' />
                        <field name="name" />
                        <field name="ref" />
                        <field name="statement_id" invisible="1" />
                        <field name="partner_id" />
                        <field
                                name="account_id"
                                options='{"no_open":True}'
                                domain="[('company_id', '=', company_id)]"
                                groups="account.group_account_user"
                            />
                        <field
                                name="analytic_account_id"
                                groups="account.group_account_user"
                            />
                        <field name="reconciled" invisible="1" />
                        <field name="full_reconcile_id" />
                        <field name="debit" sum="Total Debit" />
                        <field name="credit" sum="Total Credit" />
                        <field
                                name="amount_currency"
                                readonly="True"
                                groups="base.group_multi_currency"
                            />
                        <field name="currency_id" readonly="True" invisible="1" />
                        <field name="date_maturity" />
                        <field name="company_currency_id" invisible="1" />
                        <field name="company_id" invisible="1" />
                    </tree>
                </field>
            </xpath>
        </data>
    </field>
</record>

<record id="bukti_potong_pph_f113306_in_view_search" model="ir.ui.view">
    <field name="name">l10n_id.bukti_potong_pph_f113306_in - Search</field>
    <field name="model">l10n_id.bukti_potong_pph_f113306_in</field>
    <field name="mode">primary</field>
    <field name="priority">100</field>
    <field
            name="inherit_id"
            ref="ssi_l10n_id_taxform_bukti_potong_pph_mixin.bukti_potong_pph_mixin_search"
        />
    <field name="arch" type="xml">
        <data />
    </field>
</record>

<record id="bukti_potong_pph_f113306_in_action" model="ir.actions.act_window">
    <field name="name">Bukti Potong PPh 23 (f.1.1.33.06) In</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">l10n_id.bukti_potong_pph_f113306_in</field>
    <field name="view_mode">tree,form</field>
    <field name="search_view_id" ref="bukti_potong_pph_f113306_in_view_search" />
    <field
            name="context"
        >{'default_filter_draft':1, 'default_filter_confirm':1, 'bypass_duplicate':1}</field>
</record>

<menuitem
        id="bukti_potong_pph_23_in_menu"
        name="PPh 23 (f.1.1.33.06) In"
