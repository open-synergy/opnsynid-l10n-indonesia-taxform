<record id="bukti_potong_pph_f113306_in_view_tree" model="ir.ui.view">
    <field name="name">l10n_id.bukti_potong_pph_f113306_in - Tree</field>
    <field name="model">l10n_id.bukti_potong_pph_f113306_in</field>
    <field name="mode">primary</field>
    <field name="priority">100</field>
    <field
            name="inherit_id"
            ref="ssi_l10n_id_taxform_bukti_potong_pph_mixin.bukti_potong_pph_mixin_view_tree"
        />
    <field name="arch" type="xml">
        <data />
